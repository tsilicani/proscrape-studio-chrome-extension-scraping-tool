  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
